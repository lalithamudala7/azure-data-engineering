{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "lalithDBADF"
        },
        "dataFactory_location": {
            "type": "string",
            "defaultValue": "eastus"
        },
        "dataFactory_identity_type": {
            "type": "string",
            "defaultValue": "SystemAssigned"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "[parameters('factoryName')]",
            "type": "Microsoft.DataFactory/factories",
            "apiVersion": "2018-06-01",
            "properties": {},
            "dependsOn": [],
            "location": "[parameters('dataFactory_location')]",
            "identity": {
                "type": "[parameters('dataFactory_identity_type')]",
                "principalId": "9fc0520e-9010-4dbb-a7f7-8891c3eb9027",
                "tenantId": "88d59d7d-aecb-41b2-90c5-55595de02536"
            }
        }
    ]
}